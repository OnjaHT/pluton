{
  "name": "Pluton",
  "manifest_version": 2,
  "version": "1.0.0",
  

  "description": "Juste un petit test avec pluton",
  "default_locale": "fr",

  "icons": [
    {
        "src": "images/ico/icon-48.png",
        "sizes": "48x48",
        "type": "image/png"
    },
    {
        "src": "images/ico/icon-96.png",
        "sizes": "96x96",
        "type": "image/png"
    },
  ],

  
  "gcm_sender_id": "894523585077",

  "applications": {
    "gecko": {
      "id": "addon@example.com",
      "strict_min_version": "42.0"
    }
  },

  "permissions": [
      "webNavigation",
      "geolocation",
      "notifications",
      "storage",
      "nativeMessaging"
    ]
}